{
  "scopeName": "source.mscript",
  "patterns": [
    {
      "name": "comment.line.number-sign.mscript",
      "match": "#.*$"
    },
    {
      "name": "string.quoted.double.mscript",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "constant.numeric.mscript",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "constant.language.mscript",
      "match": "\\b(true|false|None)\\b"
    },
    {
      "name": "keyword.control.mscript",
      "match": "\\b(if|else|while|for|return|def|in|or|elif|import|try|catch|break|not)\\b"
    },
    {
      "name": "entity.name.function.definition.mscript",
      "match": "\\bdef\\s+([a-zA-Z_][a-zA-Z0-9_]*)"
    },
    {
      "name": "entity.name.function.mscript",
      "match": "(?<!\\.)\\b[a-zA-Z_][a-zA-Z0-9_]*(?=\\s*\\()"
    },
    {
      "name": "support.function.mscript",
      "match": "\\b(input|str|int|type|print|len|keys|values|bytes|encode)\\b"
    },
    {
      "name": "variable.mscript",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
    }
  ]
}
